{
  "name": "sleimanx2/plastic",
  "license": "MIT",
  "description": "Plastic is an Elasticsearch ODM and mapper for Laravel. It renders the developer experience more enjoyable while using Elasticsearch by providing a fluent syntax for mapping , querying and storing eloquent models.",
  "keywords": ["elasticsearch","elastic","laravel","eloquent","model","ODM","mapper"],
  "homepage": "https://github.com/xjqxz2/plastic",
  "authors": [
    {
      "name": "xjqxz2",
      "email": "xjqxzmxl@gmail.com",
      "homepage": "http://",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "elasticsearch/elasticsearch": "~2.2",
    "illuminate/container": "5.2.*|5.3.*",
    "illuminate/contracts": "5.2.*|5.3.*",
    "illuminate/console": "5.2.*|5.3.*",
    "illuminate/pagination": "5.2.*|5.3.*",
    "illuminate/support": "5.2.*|5.3.*",
    "illuminate/database": "5.2.*|5.3.*",
    "ongr/elasticsearch-dsl": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "^0.9.4",
    "phpunit/php-code-coverage": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Sleimanx2\\Plastic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sleimanx2\\Plastic\\Tests\\": "tests/"
    }
  }
}
